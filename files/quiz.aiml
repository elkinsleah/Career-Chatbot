<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
    <category>
        
        <!-- Career Quiz -->
        
        <pattern>quiz</pattern>
        <template>
            <delay>1</delay>
            Welcome to the Career Path Quiz! üìù<break/>
            You will be asked a series of questions about your personal interests
            and strengths to determine a career that best suits you! <break/>
            Are you ready to begin?
            
            <button>
                <text>Yes!</text>
                <postback>yes</postback>
            </button>
            
            <button>
                <text>No, return to main menu</text>
                <postback>main</postback>
            </button>
            
        </template>
    </category>
    
    <!-- Quiz Beginning -->

    <category>
        <pattern>yes</pattern>
        <template>
            
           üìù Quiz beginning: <split/>
           Do you enjoy writing code?
           
           <button>
               <text>Yes</text>
               <postback>yescode</postback>
           </button>
           
           <button>
               <text>No</text>
               <postback>nocode</postback>
           </button>
           
        </template>
    </category>
    
    <!-- Yes Code -->
    
    <category>
        <pattern>yescode</pattern>
        <template>
            
            Which would you consider one of your strengths?
            
            <button>
                <text>Creativity</text>
                <postback>creative</postback>
            </button>
            
            <button>
                <text>Attention to detail</text>
                <postback>detail</postback>
            </button>
            
        </template>
    </category>
    
    <!-- Creative -->
    
    <category>
        <pattern>creative</pattern>
        <template>
            
            Which would you rather do?
            
            <button>
                <text>Create apps, websites, or software</text>
                <postback>nogames</postback>
            </button>
            
            <button>
                <text>Create and design games</text>
                <postback>games</postback>
            </button>
            
        </template>
    </category>
    
    <!-- Software Developer -->
    
    <category>
        <pattern>nogames</pattern>
        <template>
            
            You should become a
            
            <link>
                <text>Software Developer!</text>
