<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
    
    <!-- Initial start up menu -->

<category>
    <pattern><set>greeting</set></pattern>
    <template>
        <delay>1</delay>
        👋 Hello! This is the WGU Computer Science Career Service Chatbot. 🤖
        Please select an option from below!

        <button>
            <text>📝 Take Career Path Quiz</text>
            <postback>quiz</postback>
        </button>
        
        <button>
            <text>🤔 See Career Path Options</text>
            <postback>options</postback>
        </button>
        
    </template>
</category>

<category>
    
    <!-- Return to main menu -->
    
    <pattern>main</pattern>
    <template>Main Menu:
    
    <button>
        <text>📝 Take Career Path Quiz</text>
        <postback>quiz</postback>
    </button>
    
    <button>
        <text>🤔 See Career Path Options</text>
        <postback>options</postback>
    </button>
    
    </template>
</category>
	
</aiml>